   "source": [
    "import cugrad\n",
    "a = cugrad.Tensor(2.0); a.label = 'a'\n",
    "b = cugrad.Tensor(3.0); b.label = 'b'\n",
    "c = cugrad.Tensor(1.0); c.label = 'c'\n",
    "d = a * b; d.label = 'd'\n",
    "e = d + c; e.label = 'e'\n",
    "e.backward()\n",
    "\n",
    "def dfs(t):\n",
    "    print(t)\n",
    "    for child in t.children:\n",
    "        dfs(child)\n",
    "\n",
    "dfs(e)"
   ]
  }
